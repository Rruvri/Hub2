from datetimetracking import *

class Memos:
    def __init__(self, name, memos_list=[]):
         self.name = name
         self.memos_list = memos_list
    
def create_memos_collection(memos):
    name = input("Enter title for memo list: ")
    memos.append(Memos(name))

